{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/straphone/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/straphone/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/straphone",
  "projects": {
    "base-station": {
      "type": "application",
      "root": "apps/base-station",
      "entryFile": "main",
      "sourceRoot": "apps/base-station/src",
      "compilerOptions": {
        "tsConfigPath": "apps/base-station/tsconfig.app.json"
      }
    },
    "billing": {
      "type": "application",
      "root": "apps/billing",
      "entryFile": "main",
      "sourceRoot": "apps/billing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing/tsconfig.app.json"
      }
    },
    "db": {
      "type": "library",
      "root": "libs/db",
      "entryFile": "index",
      "sourceRoot": "libs/db/src",
      "compilerOptions": {
        "tsConfigPath": "libs/db/tsconfig.lib.json"
      }
    },
    "straphone": {
      "type": "application",
      "root": "apps/straphone",
      "entryFile": "main",
      "sourceRoot": "apps/straphone/src",
      "compilerOptions": {
        "tsConfigPath": "apps/straphone/tsconfig.app.json"
      }
    }
  }
}